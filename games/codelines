LotteryDraw =
  play: ->
    result  = LotteryTicket.new_random()
    winners = {}
    this.tickets.each (buyer, ticket_list) ->
      ticket_list.each (ticket) ->
        score = ticket.score result
        return if score is 0
        winners[buyer] or= []
        winners[buyer].push [ticket, score]
    this.tickets = {}
    winners




