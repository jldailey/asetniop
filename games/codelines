total = [0,0,0]
failures = []
global.testGroup = (name, tests) ->
	failed = passed = 0
	for test_name of tests
		if test_to_run in ["*", test_name, name]
			test = tests[test_name]
			total[0] += 1
			try
				test()
				passed += 1
				total[1] += 1
			catch err
				failed += 1
				total[2] += 1
				failures.push(test_name)
				if test_to_run isnt "*"
					throw err
global.testReport = () ->

testGroup("Testing Framework"
	pass: () -> true
)
