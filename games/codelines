	"": "&para;"
	"": "&middot;"
	"": "&cedil;"
	"": "&sup1;"
	"": "&ordm;"
	"": "&raquo;"
	"": "&frac14;"
	"": "&frac12;"
	"": "&frac34;"
	"": "&iquest;"
	"": "&Agrave;"
	"": "&Aacute;"
	"": "&Acirc;"
	"": "&Atilde;"
	"": "&Auml;"
	"": "&Aring;"
	"": "&AElig;"
	"": "&Ccedil;"
	"": "&Egrave;"
	"": "&Eacute;"
	"": "&Ecirc;"
	"": "&Euml;"
	"": "&Igrave;"
	"": "&Iacute;"
	"": "&Icirc;"
	"": "&Iuml;"
	"": "&ETH;"
