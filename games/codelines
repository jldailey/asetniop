							code:
								type: "string"
								required:true
							name:
								type:"string"
								required:false
				usesFeatures:
					type:"boolean"
					required: false
		points:
			type:"array"
			required:true
			minItems:1
			items:
				type: "object"
				additionalProperties:false
				properties :
					code:
						type: "string"
						required:true
					name:
						type:"string"
						required:false
					decisions:
						type: "array"
						required:true
						minItems:1
